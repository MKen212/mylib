<!-- Main Authors - Show all Page-->
<!-- Search Author Form -->
<h2>Search Authors</h2>
<form action="/authors" method="GET">
  <label for="nameSearch">Name: </label>
  <input type="text" name="name" id="nameSearch" value="<%= searchOptions.name %>" />
  <button type="submit">Search</button>
</form>
<br />
<!-- Display Authors -->
<% authors.forEach(author => { %>
  <div><%= author.name %></div>
<% }) %>

